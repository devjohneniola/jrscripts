{
    "name": "jrscripts",
    "version": "1.0.0",
    "description": "A Classic PWA React Boiler Template that uses Material UI design and React Router for routing pages",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/devjohneniola/jrscripts.git"
    },
    "bugs": {
        "url": "https://github.com/devjohneniola/jrscripts/issues"
    },
    "homepage": ".",
    "scripts": {
        "start": "webpack serve --mode=development",
        "build": "set NODE_ENV=production && webpack --mode=production",
        "build_unix": "export NODE_ENV=production && webpack --mode=production",
        "serve": "serve -s dist -l 8000"
    },
    "author": "John Eniola",
    "maintainers": [
        {
            "name": "John Eniola",
            "email": "devjohneniola@gmail.com"
        }
    ],
    "node_engine": {
        "node": ">=14.16.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "pretty-quick --staged",
            "eslint",
            "git add"
        ]
    },
    "no-edit-menu": true,
    "window": {
        "title": "JRA",
        "position": "center",
        "show_in_taskbar": true,
        "show": true,
        "width": 1366,
        "height": 768,
        "min_width": 360,
        "min_height": 240
    },
    "keywords": [
        "Boiler Template",
        "React",
        "React Router",
        "Material UI",
        "PWA",
        "Progressive Web App",
        "Eslint",
        "Prettier",
        "Babel",
        "Webpack"
    ],
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@fontsource/roboto": "^4.5.0",
        "@mui/icons-material": "^5.0.0-rc.1",
        "@mui/lab": "^5.0.0-alpha.46",
        "@mui/material": "^5.0.0-rc.1",
        "@mui/styles": "^5.0.0-rc.1",
        "acorn": "^8.5.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/eslint-parser": "^7.15.7",
        "@babel/plugin-proposal-export-default-from": "^7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.14.5",
        "@hot-loader/react-dom": "^17.0.1",
        "babel-loader": "^8.2.2",
        "babel-plugin-import": "^1.13.3",
        "compression-webpack-plugin": "^9.0.0",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.3.0",
        "dotenv": "^10.0.0",
        "dotenv-webpack": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "mini-svg-data-uri": "^1.3.3",
        "prettier": "^2.4.1",
        "pretty-quick": "^3.1.1",
        "react-hot-loader": "^4.13.0",
        "serve": "^12.0.1",
        "style-loader": "^3.3.0",
        "webpack": "^5.53.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.2.1"
    }
}
